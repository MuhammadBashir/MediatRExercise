{
    "Website": {
        "ApiPrefix": "//localhost:56677/cars"
    },
    "Tracking": {
        "UtsUrl": "https://uts.dev.aws.abtl.io"
    },
    "Serilog": {
        "Using": [ "Serilog.Sinks.Trace" ],
        "MinimumLevel": {
            "Default": "Debug",
            "Override": {
                "SemCms": "Debug"
            }
        },
        "WriteTo": [
            { "Name": "Trace" }
        ],
        "Enrich": [
            "FromLogContext",
            "WithMachineName",
            "WithThreadId"
        ],
        "Properties": {
            "Application": "CarcomCars-Admin"
        }
    }
}